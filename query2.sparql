PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX deploy: <./deployment#> PREFIX session: <./session#> SELECT ?deployment ?type ?status ?endpoint ?lastUpdated WHERE { ?deployment a session:Deployment ; session:deploymentType ?type ; session:status ?status . OPTIONAL { ?deployment session:endpoint ?endpoint } OPTIONAL { ?deployment session:lastUpdated ?lastUpdated } }
