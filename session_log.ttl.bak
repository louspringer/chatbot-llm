    guidance:hasActor :ClaudeAI ;
    guidance:hasTimestamp "2024-03-20T18:30:00Z"^^xsd:dateTime ;
    guidance:hasChangeReason "Creating session log and preserving current session state" ;
    guidance:hasChangeContext [
        a guidance:ChangeContext ;
