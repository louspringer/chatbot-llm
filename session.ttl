    rdfs:label "Core Teams Bot Setup" ;
    session:status session:InProgress ;
    session:completedSteps [
        rdf:_1 "Directory structure setup" ;
        rdf:_2 "Basic state management" ;
        rdf:_3 "Local development environment"
    ] ;
    session:lastUpdated "2024-02-07T18:15:00Z"^^xsd:dateTime .

